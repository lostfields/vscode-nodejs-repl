{
    "name": "nodejs-repl",
    "displayName": "Node.js REPL",
    "description": "Node.js Interactive window (REPL)",
    "version": "0.5.14",
    "publisher": "lostfields",
    "preview": true,
    "engines": {
        "vscode": "^1.19.0"
    },
    "license": "MIT",
    "categories": [
        "Languages",
        "Other"
    ],
    "keywords": [
        "node",
        "node.js",
        "nodejs",
        "repl",
        "interactive"
    ],
    "icon": "icon.png",
    "bugs": {
        "url": "https://github.com/lostfields/vscode-nodejs-repl/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/lostfields/vscode-nodejs-repl.git"
    },
    "activationEvents": [
        "workspaceContains:package.json",
        "onCommand:extension.nodejsRepl",
        "onCommand:extension.nodejsReplCurrent",
        "onCommand:extension.nodejsReplClose"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.nodejsRepl",
                "title": "Node.js Interactive window (REPL)"
            },
            {
                "command": "extension.nodejsReplCurrent",
                "title": "Node.js Interactive window (REPL) at selected file"
            },
            {
                "command": "extension.nodejsReplClose",
                "title": "Stop Node.js Interactive window (REPL)"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.7.5",
        "vscode": "^1.1.36",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
